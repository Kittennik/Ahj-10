{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "class-methods-use-this": 0,
        "no-console": 0,
        "no-restricted-syntax": 0,
        "consistent-return": 0,
        "no-unused-vars": 0,
        "prefer-promise-reject-errors": 0
    }
}
